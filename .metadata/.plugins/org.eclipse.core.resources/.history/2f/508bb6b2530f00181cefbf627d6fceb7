package com.almundo.utils;

public enum Propiedad {

	OPERADOR("Operadores",10),
	SUPERVISOR("Supervisores",5),
	DIRECTOR("Directores",2),

	NUMEROLLAMADAS("Llamadas", 20);

	private String descripcion;
	private int cantidad;

	private Propiedad(String cualitativo, int cuantitativo) {
		this.descripcion= cualitativo;
		this.cantidad  = cuantitativo;
	}

	public String getCualitativo() {
		return descripcion;
	}
	public int getCuantitativo() {
		return cantidad;
	}

}
