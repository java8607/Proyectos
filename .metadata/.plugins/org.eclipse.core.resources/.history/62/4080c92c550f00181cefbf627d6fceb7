package com.almundo.utils;

public enum Propertie {

	OPERATOR("Operadores",10),
	SUPERVISOR("Supervisores",5),
	DITECTOR("Directores",2),
	CALL("Llamadas", 20);

	private String descripcion;
	private int cantidad;

	private Propertie(String cualitativo, int cuantitativo) {
		this.descripcion= cualitativo;
		this.cantidad  = cuantitativo;
	}

	public String getDescripcion() {
		return descripcion;
	}
	public int getCantidad() {
		return cantidad;
	}

}
