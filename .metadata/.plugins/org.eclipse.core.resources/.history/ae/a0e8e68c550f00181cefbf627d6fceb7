package com.almundo.utils;

public enum Property {

	OPERATOR("Operator",10),
	SUPERVISOR("Supervisor",5),
	DIRECTOR("Director",2),
	CALL("Calls", 20);

	private String description;	
	private int quantity;

	private Property(String cualitativo, int cuantitativo) {
		this.description= cualitativo;
		this.quantity  = cuantitativo;
	}

	public String getDescripcion() {
		return description;
	}
	public int getCantidad() {
		return quantity;
	}

}
