package com.almundo.utils;

public enum Propiedad {

	NUMEROOPERADORES("Cantidad operadores",10),
	NUMEROSUPERVISORES("Cantidad supervisores",5),
	NUMERODIRECTORES("Cantidad directores",2),

	NUMEROLLAMADAS("Cantidad llamadas", 20);

	private String cualitativo;
	private int cuantitativo;

	private Propiedad(String cualitativo, int cuantitativo) {
		this.cualitativo= cualitativo;
		this.cuantitativo  = cuantitativo;
	}

	public String getCualitativo() {
		return cualitativo;
	}
	public int getCuantitativo() {
		return cuantitativo;
	}

}
